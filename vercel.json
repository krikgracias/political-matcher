{
  "routes": [
    {
      "src": "/elections/([^/]+)/([^/]+)/([^/]+)",
      "dest": "/?state=$1&county=$2&office=$3"
    },
    {
      "src": "/elections/([^/]+)/([^/]+)/ballot-measures-([^/]+)",
      "dest": "/?state=$1&county=$2&office=ballot-measures&year=$3"
    },
    {
      "src": "/elections",
      "dest": "/elections-index"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}